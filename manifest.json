{
	"name": "jsFiddle",
	"version": "2.0.0",
	"description": "View your fiddles with their revisions and create fiddles directly from the extension.",
	"icons": {
		"16": "icons/icon-16.png",
		"48": "icons/icon-48.png",
		"128": "icons/icon-128.png"
	},
	"browser_action": {
		"default_icon": "assets/images/icon.png",
		"popup": "popup.html"
	},
	"permissions": [
		"http://jsfiddle.net/"
	]
}
